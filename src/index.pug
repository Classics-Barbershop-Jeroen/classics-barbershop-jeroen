// Contact Block
section#contact.contact-section.h-100.bg-dark
  #map.bg-overlay
  .container.py-5
    .col-lg-8.col-md-6.col-sm-10.form-sec.bg-white.my-5.p-5.mx-auto
      form#mailform(action='https://jumprock.co/mail/infocbj' method='post')
        h2.mb-4 Contacteer ons
        .row
          .col-md-6
            .form-group
              label.sr-only(for='formGroupExampleInput') Volledige naam
              input#formGroupExampleInput.form-control(name='name' placeholder='Volledige naam' onfocus="this.placeholder=''" onblur="this.placeholder='Volledige naam'" type='text')
          .col-md-6
            .form-group
              label.sr-only(for='telefoonnummer') Telefoonnummer
              input#telefoonnummer.form-control(name='phone' placeholder='Telefoonnummer' onfocus="this.placeholder=''" onblur="this.placeholder='Telefoonnummer'" type='text')
        .row
          .col-md-6
            .form-group
              label.sr-only(for='emailadres') Email-adres
              input#emailadres.form-control(name='email' placeholder='Email-adres' onfocus="this.placeholder=''" onblur="this.placeholder='Email-adres'" type='email')
          .col-md-6
            .form-group
              //
                <label class="sr-only" for="adres">Adres</label>
                <input class="form-control" id="adres" placeholder="Adres" onfocus="this.placeholder=''" onblur="this.placeholder='Enter Address'" type="email">
        .row
          .col-md-12
            .form-group
              label.sr-only Bericht
              textarea.form-control(name='message' placeholder='Typ uw bericht hier!' rows='4' onfocus="this.placeholder=''" onblur="this.placeholder='Typ uw bericht hier!'")
            .form-group
              button.btn.btn-primary.btn-capsul.px-4.g-recaptcha(style='float:right;' data-sitekey='6LcV37kUAAAAAI1TLnykCL2LonSqp6hmjsKdaeyD' data-callback='onSubmit' data-badge='inline' data-size='0')
                | Verzenden
        input(name='subject' value='Contactformulier' style='display:none')
        input(type='text' name='trapit' value='' style='display:none')
        input(type='hidden' name='after' value='https://classicsbarbershopjeroen.nl/')
